                         xmlns:adtcore="http://www.sap.com/adt/core">
  <chkl:messages>
    <msg objDescr="Class ZCL_TEST_FLIGHT"
         type="W"
         line="0"
         href="/sap/bc/adt/oo/classes/zcl_test_flight"
         forceSupported="false">
